<div class="container">
	<p id="notice"><%= notice %></p>

	<p>
	  <strong>Title:</strong>
	  <%= @link.title %>
	</p>

	<p>
	  <strong>Url:</strong>
	  <%= @link.url %>
	</p>
	<% if @link.user == current_user %>
		<%= link_to 'Edit', edit_link_path(@link) %> |
		<%= link_to 'Back', links_path %>
	<% end %>
</div>